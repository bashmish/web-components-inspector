{
  "manifest_version": 2,
  "name": "web-components-inspector",
  "version": "0.0.1",
  "description": "Web Components inspector (supports Polymer)",
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/publisher/page-injector.js"]
    }
  ],
  "web_accessible_resources": ["node_modules/rempl/dist/rempl.js", "src/publisher/index.js"]
}
