{
  "manifest_version": 2,
  "name": "Web Components Inspector",
  "version": "0.0.2",
  "description": "Web Components Inspector Chrome extension (supports Polymer)",
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["page-injector.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": ["publisher.js", "subscriber.js"]
}
